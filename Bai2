#include <stdio.h>
#include <math.h>
int isPerfectSquare(int n) 
{
    int sqrtN = sqrt(n);
    return (sqrtN * sqrtN == n);
}
int main(void)
{
    // Your code here!
    int n;
    int dem=0;
    printf("Nháº­p n: ");
    scanf("%d", &n);
     printf("Cac so chinh phuong la:");
    for(int i=1;i<n;i++)
    {
        if(isPerfectSquare(i))
        {
            dem++;
           
            printf("%d \t",i);
        }
    }
    printf("Tong so chinh phuong la:%d",dem);
    
    return 0;
}

